{
	"manifest_version": 3,
	"name": "Epoch Converter",
	"description": "Showing in the context menu a human readable date of a highlighted Unix timestamp.",
	"version": "2.0.0",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		"contextMenus"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": [],
			"js": [
				"selection.js"
			],
			"all_frames": true
		}
	]
}